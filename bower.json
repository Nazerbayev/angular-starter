{
  "name": "angular-setup",
  "description": "Gulp setup with bower and browserify for angularjs projects",
  "main": "gulpfile.js",
  "authors": [
    "William Bettencourt"
  ],
  "license": "MIT",
  "moduleType": [],
  "homepage": "",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
